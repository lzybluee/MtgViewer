            android:hint="ccolors"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/common_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Common"
            android:hint="common" />

        <Button
            android:id="@+id/uncommon_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
