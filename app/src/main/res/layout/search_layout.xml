            android:layout_height="wrap_content"
            android:text="Name"
            android:hint="name" />

        <Button
            android:id="@+id/flavor_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Flavor"
            android:hint="flavor" />

