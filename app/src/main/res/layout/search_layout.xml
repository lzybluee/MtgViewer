<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222222"
    android:orientation="vertical" >

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/init_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Init" />

        <Button
            android:id="@+id/search_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Search" />

        <Button
            android:id="@+id/sort_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Edition" />

        <Button
            android:id="@+id/unique_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1+" />

        <Button
            android:id="@+id/help_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Help" />

        <Button
            android:id="@+id/clear_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Clear" />

    </LinearLayout>

    <TextView
        android:text="Output"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/output" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Code here"
        android:ems="10"
        android:id="@+id/code" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/search_name_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" @ "
            android:hint="@" />

        <Button
            android:id="@+id/return_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="return"
            android:hint="return " />

        <Button
            android:id="@+id/space_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="space"
            android:hint=" " />

        <Button
            android:id="@+id/left_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" ( "
            android:hint="(" />

        <Button
            android:id="@+id/right_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" ) "
            android:hint=")" />

        <Button
            android:id="@+id/and_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="and"
            android:hint=" and " />

        <Button
            android:id="@+id/or_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="or"
            android:hint=" or "/>

        <Button
            android:id="@+id/not_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="not"
            android:hint="not " />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/equal_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" == "
            android:hint=" == " />

        <Button
            android:id="@+id/greater_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" &gt; "
            android:hint=" &gt; " />

        <Button
            android:id="@+id/lesser_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" &lt; "
            android:hint=" &lt; " />

        <Button
            android:id="@+id/greater_equal_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" &gt;= "
            android:hint=" &gt;= " />

        <Button
            android:id="@+id/lesser_equal_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" &lt;= "
            android:hint=" &lt;= " />

        <Button
            android:id="@+id/not_equal_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" != "
            android:hint=" != " />

        <Button
            android:id="@+id/quote_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" &quot; "
            android:hint="&quot;" />

        <Button
            android:id="@+id/apos_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" &apos; "
            android:hint="&apos;" />

        <Button
            android:id="@+id/comma_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" , "
            android:hint=" , " />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/white_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="White"
            android:hint="w" />

        <Button
            android:id="@+id/blue_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Blue"
            android:hint="u" />

        <Button
            android:id="@+id/black_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Black"
            android:hint="b" />

        <Button
            android:id="@+id/red_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Red"
            android:hint="r" />

        <Button
            android:id="@+id/green_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Green"
            android:hint="g" />

        <Button
            android:id="@+id/colors_button"
