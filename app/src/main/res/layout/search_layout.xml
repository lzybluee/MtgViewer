            android:hint="colors" />

        <Button
            android:id="@+id/multicolor_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="M"
            android:hint="m" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


        <Button
            android:id="@+id/text_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Text"
            android:hint="text" />

        <Button
            android:id="@+id/mana_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mana"
            android:hint="mana" />

        <Button
            android:id="@+id/value_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Value"
            android:hint="mv" />

        <Button
            android:id="@+id/name_button"
